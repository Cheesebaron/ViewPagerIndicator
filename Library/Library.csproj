<Project Sdk="MSBuild.Sdk.Extras">
  <PropertyGroup>
    <TargetFrameworks>monoandroid90;monoandroid10.0</TargetFrameworks>
    <AssemblyName>DK.Ostebaronen.Droid.ViewPagerIndicator</AssemblyName>
    <RootNamespace>DK.Ostebaronen.Droid.ViewPagerIndicator</RootNamespace>
    <PackageId>ViewPagerIndicator</PackageId>
    <Description>A port of ViewPagerIndicator for Xamarin.Android. A highly customizable indicator for ViewPager.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE" Pack="true" PackagePath="" />
    <AndroidResource Include="Resources\**\*.xml" />
    <AndroidResource Include="Resources\**\*.axml" />
    <AndroidResource Include="Resources\**\*.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Xamarin.Android.Support.ViewPager" Version="28.0.0.3" />
  </ItemGroup>

</Project>